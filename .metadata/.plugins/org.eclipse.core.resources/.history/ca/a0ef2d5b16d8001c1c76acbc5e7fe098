package com.spring.Bean_autowiring;

import org.springframework.stereotype.Component;

@Component
public class Tyre {
	
	public Tyre(String brand) {
		super();
		this.brand = brand;
	}

	String brand;

	public String getBrand() {
		return brand;
	}

	public void setBrand(String brand) {
		this.brand = brand;
	}

	@Override
	public String toString() {
		return "| Tyre : " + brand;
	}
}
